template data_normal_health_named{
    with entity_type:word attack_damage:float move_speed:float entity_health:float entity_name:raw{
            data merger entity @e[type=<%entity_type%>,tag=!nightmare,limit=1] {Health:<%entity_health%>f,Attributes:[{Base:<%attack_damage%>d,Name:"generic.attack_damage"},{Base:<%move_speed%>d,Name:"generic.movement_speed"}],Tags:["nightmare"]}
    }
}